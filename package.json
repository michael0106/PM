{
  "name": "digital-affiliate-portal",
  "version": "1.0.0",
  "description": "Digital Affiliate Portal (DAP) - CPA业务支持系统",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend && npm run build",
    "test": "npm run test:frontend && npm run test:backend",
    "test:frontend": "cd frontend && npm run test",
    "test:backend": "cd backend && npm run test",
    "lint": "npm run lint:frontend && npm run lint:backend",
    "lint:frontend": "cd frontend && npm run lint",
    "lint:backend": "cd backend && npm run lint",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "setup": "npm install && cd frontend && npm install && cd ../backend && npm install"
  },
  "keywords": [
    "CPA",
    "affiliate",
    "portal",
    "digital",
    "business"
  ],
  "author": "DAP Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/michael0106/PM.git"
  },
  "bugs": {
    "url": "https://github.com/michael0106/PM/issues"
  },
  "homepage": "https://github.com/michael0106/PM#readme"
}